{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "robotframework-lsp",
            "name": "Robot Framework: Launch .robot file",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "target": "${file}",
            "terminal": "integrated",
            "env": {},
            "args": [
                "--outputdir",
                "output"
            ]
        },
        {
            "type": "robotframework-lsp",
            "name": "Robot Framework: Launch template",
            "request": "launch",
            "args": [
                "--outputdir",
                "output"
            ]
        }
    ]
}